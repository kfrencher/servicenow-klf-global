        <operation_script><![CDATA[(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    var body = request.body;
    var bodyJSON = body.dataString;

    /** 
     * Refer to {@link global.KLF_RecordSync_RoleUtils.getRemoteRoleAssignedTo}. That function generates
     * the request object that is expected here.
     * @type {{roleNames:string[]}} 
     */
    var requestData = JSON.parse(bodyJSON);
    var result =
        global.KLF_RecordSync_GroupUtils.getRemoteRoleAssignedTo(requestData.roleNames);

    response.setStatus('200');
    response.setBody(result);

})(request, response);]]></operation_script>
